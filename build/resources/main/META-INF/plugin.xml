<idea-plugin>
  <idea-version since-build="193.5662" until-build="193.*"/>
  <id>com.your.company.unique.plugin.id</id>
  <name>TranslateToDoc</name>
  <version>1.0-SNAPSHOT</version>
  <vendor email="jiahangchun@163.com" url="https://github.com">jiahangchun</vendor>
  <description>
      Automatically generate some targeted documents.&lt;br&gt;
    </description>
  <change-notes>
      Add change notes here.&lt;br&gt;
      &lt;em&gt;most HTML tags may be used&lt;/em&gt;</change-notes>
  <depends>com.intellij.modules.platform</depends>
  <extensions defaultExtensionNs="com.intellij"/>
  <actions>
    <group id="TP_Group" text="TP" description="Self function">
      <add-to-group group-id="MainMenu" anchor="last"/>
      <action id="TranslateDoc.TranslateDocAction" class="com.tp.TranslateDocAction" text="接口文档转换" description="Translate swagger to doc"/>
    </group>
  </actions>
</idea-plugin>
